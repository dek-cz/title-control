{
  "name": "dek-cz/title-control",
  "type": "library",
  "license": "BSD-3-Clause",
  "description": "Nette control for managing title in HTML header.",
  "keywords": ["title", "header", "component", "nette"],
  "support": {
    "email": "jan.zaruba@dek-cz.com",
    "issues": "https://github.com/dek-cz/title-control/issues"
  },
  "authors": [
    {
      "name": "Jan ZÃ¡ruba",
      "email": "jan.zaruba@dek-cz.com"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "nette/utils": "^3.1.3@dev",
    "nette/application": "^3.0@dev"
  },
  "autoload": {
    "psr-4": {
      "Vrestihnat\\TitleControl\\": "src/TitleControl/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VrestihnatTests\\": "tests/"
    }
  },
  "config": {
    "platform-check": false
